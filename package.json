{
  "name": "express-typescript-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:NurfitraPujo/express-typescript-boilerplate.git",
  "author": "Nurfitra Pujo Santiko <nurfitrapujo.19@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist && rimraf dist-webpack && rimraf dist-babel && rimraf dist-tsc && rimraf reports && rimraf build && rimraf tsconfig.tsbuildinfo",
    "lint": "npm run pretty && eslint -c ./.eslintrc.js ./src/*.ts ./src/**/*.ts ./src/**/**/*.ts ./src/**/**/**/*.ts ./src/**/**/**/**/*.ts --color --fix",
    "pretty": "prettier ./dist/*.js ./src/*.ts ./src/**/*.ts ./src/**/**/*.ts ./src/**/**/**/*.ts ./src/**/**/**/**/*.ts  --color --write",
    "build:babel": "NODE_ENV=production babel --config-file ./babel.config.js ./src --extensions .ts --out-dir ./dist-babel",
    "build:tsc": "tsc --build",
    "build:webpack": "NODE_ENV=production webpack --config=webpack.config.js --mode=production --color",
    "build": "npm run clean && run-p -l build:*",
    "type-check": "tsc --traceResolution",
    "dev": "nodemon --inspect ./src/index.ts",
    "start": "node ."
  },
  "dependencies": {
    "@babel/runtime": "^7.13.7",
    "@babel/runtime-corejs3": "^7.13.7",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "class-validator": "^0.13.1",
    "compression": "^1.7.4",
    "core-js": "^3.9.0",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "csurf": "^1.11.0",
    "es6-promise": "^4.2.8",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "validator": "^13.5.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@babel/plugin-transform-modules-commonjs": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.7",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.33",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-jsdoc": "^15.4.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "file-loader": "^6.2.0",
    "husky": "^5.1.1",
